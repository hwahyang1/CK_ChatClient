{
	"$schema": "https://json.schemastore.org/prettierrc",
	"semi": true,
	"tabWidth": 4,
	"useTabs": true,
	"singleQuote": true,
	"printWidth": 100,
	"quoteProps": "consistent",
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "crlf",
	"prettier.disableLanguages": [
		"html",
		"vue"
	]
}